  body
    .container-contact100
      .wrap-contact100
        form.contact100-form.validate-form
          span.contact100-form-title
            | Post Your Project
          .wrap-input100.validate-input(data-validate='Name is required')
            span.label-input100 Project Name :
            input.input100(type='text', name='project_name', placeholder='Enter your project name.......')
            span.focus-input100
          .wrap-input100.validate-input(data-validate='Small Discription is required')
            span.label-input100 Small Discription :
            textarea.input100(name='small_message', placeholder='Enter a small disciption which is viewed in running projects')
            span.focus-input100
          .wrap-input100.validate-input(data-validate='Full Discription is required')
            span.label-input100 Full Discription :
            textarea.input100(name='full_message', placeholder='Enter a small disciption which is viewed in running projects')
            span.focus-input100
          .wrap-input100.validate-input(data-validate='Team Name is required')
            span.label-input100 Team Name :
            input.input100(type='text', name='team_name', placeholder='Team Name.......')
            span.focus-input100
          .wrap-input100.validate-input(data-validate='Team Leader Name is required')
            span.label-input100 Team Leader Name :
            input.input100(type='text', name='leader_name', placeholder='Team Leader Name.......')
            span.focus-input100
          .wrap-input100.validate-input(data-validate='Contact Number is required')
            span.label-input100 Contact Number :
            input.input100(type='number', name='number', placeholder='Mobile Number')
            span.focus-input100
          span.contact100-form-title
            | Team Details
